{
  "extends": [
    "config:recommended",
    ":automergeMinor",
    ":automergePr"
  ],
  "packageRules": [
    {
      "groupName": "akka",
      "matchPackageNames": [
        "com.typesafe.akka:akka-{/,}**"
      ]
    },
    {
      "groupName": "zxing",
      "matchPackageNames": [
        "com.google.zxing:{/,}**"
      ]
    },
    {
      "groupName": "telegram",
      "matchPackageNames": [
        "com.bot4s:telegram{/,}**"
      ]
    }
  ]
}
